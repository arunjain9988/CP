t\t\t\t} else {\n\t\t\t\t\t$( \"#vote-\" + type + \"-box\" ).fadeOut(1500, function() {\n\t\t\t\t\t\t$(this).remove();\n\t\t\t\t\t});\n\t\t\t\t}\n\t\t\t\t*/\n\t\t\t\tvar n = $('.vote-thank-you').length;\n\t\t\t\tvar offset = (n * 60).toString() + \"px\";\n\t\t\t\t$(document.body).append(\n\t\t\t\t\t'<div class=\"vote-thank-you\" style=\"margin: 40%;\">' +\n\t\t\t\t\t'<div class=\"alert alert-success\" style=\"position: fixed; top: '+offset+';\">' +\n\t\t\t\t\t'<strong>Thank you for voting!</strong>' +\t\t\t\t\n\t\t\t\t\t'</div></div>');\n\t\t\t\tsetTimeout(function(){$( \".vote-thank-you\" ).fadeOut(1500, function() {\n\t\t\t\t\t$(this).remove();\n\t\t\t\t});},2000);\n\t\t\t}\n\t\t</script>\n\t\t\n\t\t\n\t</div><!-- endof col-lg-4 col-md-4 -->\n</div>\n\n\n\n\n\n\n<style style=\"display: none !important;\">\nul.list-unstyled li{\n\tmargin-bottom: 5px;\n}\nul.list-unstyled li a.btn{\n\twidth: 100%;\n}\nul.ui-autocomplete{\n\tz-index: 10000;\n\twidth: 200px !important;\n}\n</style>\n\t<br style=\"display: none !important;\">\n</div>\n\t\t\n\t\t\n\t\t<div style=\"display: none !important;\" id=\"footer\" class=\"container\">\n\t\t\t<div class=\"row\" style=\"display: none !important;\">\n\t\t\t\t<div class=\"col-md-10\" style=\"display: none !important;\">\n\t\t\t\t\t<p style=\"display: none !important;\"><a href=\"https://www.spoj.com/info\" style=\"display: none !important;\">About</a> | <a href=\"https://www.spoj.com/tutorials\" style=\"display: none !important;\">Tutorial</a> | <a href=\"https://www.spoj.com/tools\" style=\"display: none !important;\">Tools</a> | <a href=\"https://www.spoj.com/clusters\" style=\"display: none !important;\">Clusters</a> | <a href=\"https://www.spoj.com/credits\" style=\"display: none !important;\">Credits</a> | <a style=\"display: none !important;\" href=\"https://www.spoj.com/sphereengine\">API</a> | <a style=\"display: none !important;\" href=\"https://www.spoj.com/sphereengine-widget\">Widgets</a></p>\n\t\t\t\t\t<p style=\"displa